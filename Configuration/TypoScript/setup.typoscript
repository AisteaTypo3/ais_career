plugin.tx_aiscareer {
  view {
    templateRootPath = EXT:ais_career/Resources/Private/Templates/
    partialRootPath = EXT:ais_career/Resources/Private/Partials/
    layoutRootPath = EXT:ais_career/Resources/Private/Layouts/
  }
  settings {
    itemsPerPage = {$plugin.tx_aiscareer.settings.itemsPerPage}
    listPid = {$plugin.tx_aiscareer.settings.listPid}
    detailPid = {$plugin.tx_aiscareer.settings.detailPid}
    enableFilters = {$plugin.tx_aiscareer.settings.enableFilters}
    filterCountry = {$plugin.tx_aiscareer.settings.filterCountry}
    filterDepartment = {$plugin.tx_aiscareer.settings.filterDepartment}
    filterContractType = {$plugin.tx_aiscareer.settings.filterContractType}
    filterRemotePossible = {$plugin.tx_aiscareer.settings.filterRemotePossible}
    filterCategory = {$plugin.tx_aiscareer.settings.filterCategory}
    mapEnabled = {$plugin.tx_aiscareer.settings.mapEnabled}

    applicationEnabled = {$plugin.tx_aiscareer.settings.applicationEnabled}
    applicationToEmail = {$plugin.tx_aiscareer.settings.applicationToEmail}
    applicationFromEmail = {$plugin.tx_aiscareer.settings.applicationFromEmail}
    applicationSave = {$plugin.tx_aiscareer.settings.applicationSave}
    applicationDoubleOptInEnabled = {$plugin.tx_aiscareer.settings.applicationDoubleOptInEnabled}
    applicationConfirmationEnabled = {$plugin.tx_aiscareer.settings.applicationConfirmationEnabled}
    maxUploadSizeMB = {$plugin.tx_aiscareer.settings.maxUploadSizeMB}
    maxTotalUploadSizeMB = {$plugin.tx_aiscareer.settings.maxTotalUploadSizeMB}
    allowedExtensions = {$plugin.tx_aiscareer.settings.allowedExtensions}
    privacyPageUid = {$plugin.tx_aiscareer.settings.privacyPageUid}
    hiringOrganizationName = {$plugin.tx_aiscareer.settings.hiringOrganizationName}

    gdprPurgeDays = {$plugin.tx_aiscareer.settings.gdprPurgeDays}
    botMinSeconds = {$plugin.tx_aiscareer.settings.botMinSeconds}
    botMaxSeconds = {$plugin.tx_aiscareer.settings.botMaxSeconds}
    botRateLimit = {$plugin.tx_aiscareer.settings.botRateLimit}
    botRateWindowSeconds = {$plugin.tx_aiscareer.settings.botRateWindowSeconds}
    botRequireHeaders = {$plugin.tx_aiscareer.settings.botRequireHeaders}
  }
}

page.includeCSS.aiscareer = EXT:ais_career/Resources/Public/Css/aiscareer.css
page.includeJSFooter.aiscareer = EXT:ais_career/Resources/Public/JavaScript/aiscareer.js
page.includeJSFooter.aiscareer.defer = 1
